<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

<% include ../partials/nav.ejs %>

<div class="container">
<h2>Database Results</h2>

<ul>
    <% Object.keys(questions).forEach(function(q) { %>
        <li><%= q.id %> - <%= q.title %></li>
        <ul>
          <% q.answers.forEach(function(a) { %>
            <li><%= a.id %> - <%= a.title %></li>
          <% }); %>
        </ul>
    <% }); %>
</ul>

</div>

</body>
</html>
